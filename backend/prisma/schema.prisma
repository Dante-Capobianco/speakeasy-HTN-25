generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
    id Int @id @default(autoincrement())
    topics String[]
    practiceRuns PracticeRun[]
}

model PracticeRun {
    id Int @id @default(autoincrement())
    topics String[]
    questions String[]
    timeToReadQuestion String
    timeToAnswerQuestion String
    totalScore Int[]

    verbalScore Int[]
    relevanceScore Int[]
    structureClarityScore Int[]
    insightsScore Int[]
    vocabScore Int[]
    fillerWordScore Int[]

    nonVerbalScore Int[]
    facialExpressionScore Int[]
    eyeMovementsScore Int[]
    pausingScore Int[]
    postureScore Int[]
    handGesturesScore Int[]
    spatialDistributionScore Int[]

    posVerbalFeedback String[]
    negVerbalFeedback String[]
    posNonverbalFeedback String[]
    negNonverbalFeedback String[]
    user User @relation(fields: [userId], references: [id])
    userId Int
    videos String[]
}