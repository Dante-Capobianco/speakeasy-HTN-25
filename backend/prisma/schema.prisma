generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
    id Int @id @default(autoincrement())
    topics String[]
    practiceRuns PracticeRun[]
}

model PracticeRun {
    id Int @id @default(autoincrement())
    topics String[]
    questions String[]
    timeToReadQuestion Int
    timeToAnswerQuestion Int
    totalScore Int
    verbalScore Int
    nonVerbalScore Int
    posVerbalFeedback String[]
    negVerbalFeedback String[]
    posNonverbalFeedback String[]
    negNonverbalFeedback String[]
    user User @relation(fields: [userId], references: [id])
    userId Int
    videos String[]
}